// go to couch_east
robot.goTo("table_northeast");   // other places: door,  home, trash, couch_west, vivek_desk,
							// table_northwest, table_northeast, table_southwest, table_southeast

// display message
robot.displayMessage('Hello!', 'My Name is LoggerHead.');

// say message
robot.say("Hello, my name is Loggerhead.");

// wait for two seconds
waitForDuration(2);

// say message
robot.say("How are you?");

// Ask a question
item = robot.askMultipleChoice('How are you?', ['Good', 'Bad']);

// Display a message by adding two strings together
robot.displayMessage(String('You Selected ) + String(item));

// say message
robot.say(String('You Selected') + String(item));


// wait for two second
waitForDuration(2);

// Ask a question
item = robot.askMultipleChoicd( 'Are you enjoying your time at Do-It?' , [ 'Yes, very much' , 'Kind of' , 'No' ]);

// Display a message by adding two strings together
robot.displayMessage(String('You Selected ) + String(item));

// say message
robot.say(String('You Selected') + String(item));

// wait for two second
waitForDuration(2);

// say message
robot.say 'Have a nice day!'

// Display a message
robot.displayMessage("Bye!");

// say message
robot.say("Bye!");

// Go back to home
robot.goTo("home");









