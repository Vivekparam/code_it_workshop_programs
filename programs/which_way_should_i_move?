robot.say("Which way should I move? Left, Right, Forward, or Back?")
direction = robot.askMultipleChoice('Which way should I move?', ['Left', 'Right', 'Forward', 'Back']);
if (direction == 'Left') {
  robot.say("You chose Left")
  robot.turnLeft();
} else if (direction == 'Right') {
  robot.say("You chose Right")
  robot.turnRight(); 
} else if (direction == 'Forward') {
  robot.say("You chose Forward")
  robot.moveForward();   
} else if (direction == 'Back') {
  robot.say("You chose Back")
  robot.moveBack(); 
}
