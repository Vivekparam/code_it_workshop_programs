// go to couch_east
robot.goTo("table_northeast");   // other places: door,  home, trash, couch_west, vivek_desk,
							// table_northwest, table_northeast, table_southwest, table_southeast

// display message
robot.displayMessage('Hello!', 'My Name is LoggerHead.');

// say message
robot.say("Hello, my name is Loggerhead.");

// wait for two seconds
waitForDuration(2);

// say message
robot.say("What's your favorite team?");

// Ask a question
item = robot.askMultipleChoice('What\'s your favorite team?', ['Seahawks', 'Mariners']);

// Display a message by adding two strings together
robot.displayMessage(String('You Selected ') + String(item));

// say message
robot.say(String('You Selected ') + String(item));

// wait for one second
waitForDuration(2);

// Display a message
robot.displayMessage("See you tomorrow!");

// say message
robot.say("See you tomorrow!");

// Go back to home
robot.goTo("home");









